nsole.log(\"chosen\",name);\r\n            this.recorder[name]=1\r\n        },\r\n        doChange(){\r\n            if(this.show){//创建一个boss对象数组\r\n                this.bosses.forEach((item)=>{\r\n                    let obj={};\r\n                    obj[item.name]=0;\r\n                    this.recorder = Object.assign(this.recorder,obj)\r\n                })\r\n            }else{//清空对象数组\r\n                this.recorder={}\r\n            }\r\n            this.show=!this.show\r\n        },\r\n        upload(){\r\n            // axios.get(\"http://localhost:8888/upRecorder\",{params:{\r\n            //     recorder:this.recorder\r\n            // }}).then((response)=>{\r\n            //     alert(\"上传成功\")\r\n            // })\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            bosses: require(\"../bossData.json\").boss,\r\n            show: true,\r\n            recorder:{}\r\n        };\r\n    },\r\n    mounted() {\r\n        let preUrl = \"/boss_sprites/statue/statue_\";\r\n        axios.get(\"http://localhost:8888/missTimes\").then((response) => {\r\n            let missTimes = response.data[0];\r\n            console.log(\"missTimes=\", missTimes);\r\n            this.bosses.forEach(function (item, index) {\r\n                item.index = index;\r\n                item.iconSrc = preUrl + item.name + \".png\";\r\n                item.hitTimes = missTimes[item.name];\r\n            });\r\n            console.log();\r\n        });\r\n    },\r\n    components: { StatueCell, TimeSelector }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    .opCell{\r\n        width: 400px;\r\n        height: 100px;\r\n        background-color: #ffffff;\r\n        margin:auto;\r\n        display: flex;\r\n    }\r\n    .operations{\r\n        width: 100%;\r\n        height: 120px;\r\n        background-color: #7b7777;\r\n        display: flex;\r\n        flex-direction: row;\r\n    }\r\n    .change{\r\n        width: 30px;\r\n        height: 400px;\r\n        background-color: #7b7777;\r\n        position: absolute;\r\n        top: calc(50% - 200px);\r\n        left: 30px;\r\n    }\r\n    .change:hover{\r\n        cursor: pointer;\r\n    }\r\n    .hitTimesText{\r\n        margin: 0 auto; \r\n        width:60px;\r\n        text-align:center;\r\n        font:italic 1em Georgia, serif;\r\n        border: solid black;\r\n        border-width: 2px 0 0 0;\r\n    }\r\n    .bossNameText{\r\n        text-align:center;\r\n        font:italic 1em Georgia, serif;\r\n        padding-bottom: 10px;\r\n    }\r\n    .statueShell{\r\n        /* width:160px;\r\n        display: flex;\r\n        flex-direction: column;\r\n        user-select: none; */\r\n    }\r\n    .quit{\r\n        float: right;\r\n        width: 30px;\r\n        height: 30px;\r\n        margin: 10px;\r\n        transition: 0.3s;\r\n        /* background-color: black; */\r\n    }\r\n    .quitIcon{\r\n        width: 100%;\r\n        height: auto;\r\n    }\r\n    .quit:hover{\r\n        transform: scale(1.1);\r\n        cursor:pointer;\r\n        transform:rotate(150deg);\r\n    }\r\n    .dialogShell{\r\n        position: fixed;\r\n        top: calc(50% - 300px);\r\n        left: calc(50% - 500px);\r\n        margin:0;\r\n        width: 1100px;\r\n        height: 600px;\r\n        background-color: #ffffff;\r\n        z-index: 5;\r\n        transition: 0.5s;\r\n    }\r\n    .main{\r\n        position: relative;\r\n        top: 25px;\r\n        left: calc(50% - 450px);\r\n        background-color: #ce9191;\r\n        width: 900px;\r\n        height: 550px;\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        justify-content: center;\r\n        overflow-x:hidden;\r\n        overflow-y:show;\r\n        transition: 0.5s;\r\n    }\r\n    .statue{\r\n        width: 120px;\r\n        height: 120px;\r\n        margin: 20px;\r\n        margin-top: 50px;\r\n        background-color: #7b7777;\r\n        display: flex;\r\n        justify-content: flex-end;\r\n        flex-direction: column;\r\n    }\r\n</style>"]}]}