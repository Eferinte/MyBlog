A,aAAO,KAAP;AACF;AATO,GAnGI;AA8Gb,EAAA,IA9Ga,kBA8GN;AACL,WAAO;AACL,MAAA,kBAAkB,EAAC,CADd;AAEL,MAAA,OAAO,EAAC,CAFH;AAGL,MAAA,OAAO,EAAC,CAHH;AAIL,MAAA,SAAS,EAAC,KAJL;AAKL,MAAA,QAAQ,EAAC,EALJ;AAML,MAAA,QAAQ,EAAC,EANJ;AAOL,MAAA,MAAM,EAAC,KAPF;AAQL,MAAA,MAAM,EAAC;AACL,QAAA,KAAK,EAAC,KADD;AAEL,QAAA,QAAQ,EAAC;AAFJ,OARF;AAYL,MAAA,KAAK,EAAC;AACJ,QAAA,OAAO,EAAC,IADJ;AAEJ,QAAA,WAAW,EAAC,IAFR;AAGJ,QAAA,QAAQ,EAAC,IAHL;AAIJ,QAAA,SAAS,EAAC,IAJN;AAKJ,QAAA,aAAa,EAAC,IALV;AAMJ,QAAA,UAAU,EAAC,IANP;AAOJ,QAAA,SAAS,EAAC,IAPN;AAQJ,QAAA,aAAa,EAAC,IARV;AASJ,QAAA,UAAU,EAAC;AATP,OAZD;AAuBL,MAAA,QAAQ,EAAC;AACP,QAAA,eAAe,EAAC,GADT;AAEP,QAAA,mBAAmB,EAAC,CAFb;AAGP,QAAA,gBAAgB,EAAC,GAHV;AAIP;AACA,QAAA,kBAAkB,EAAC,CALZ;AAMP,QAAA,iBAAiB,EAAC,CANX;AAOP,QAAA,cAAc,EAAC;AAPR,OAvBJ;AAgCL,MAAA,YAAY,EAAC,KAhCR;AAiCL,MAAA,UAAU,EAAC,KAjCN;AAkCL,MAAA,YAAY,EAAC,MAlCR;AAmCL,MAAA,MAAM,EAAC,OAAO,CAAC,iBAAD,CAAP,CAA2B,IAnC7B;AAoCL,MAAA,YAAY,EAAC,EApCR;AAqCL,MAAA,QAAQ,EAAC,CACP;AACI,QAAA,KAAK,EAAC,YADV;AAEI,QAAA,KAAK,EAAC,YAFV;AAGI,QAAA,GAAG,EAAC,KAHR;AAII,QAAA,IAAI,EAAC,CAAC,MAAD,EAAQ,MAAR,EAAe,MAAf;AAJT,OADO,EAOP;AACI,QAAA,KAAK,EAAC,YADV;AAEI,QAAA,KAAK,EAAC,YAFV;AAGI,QAAA,GAAG,EAAC,KAHR;AAII,QAAA,IAAI,EAAC,CAAC,MAAD,EAAQ,MAAR,EAAe,MAAf;AAJT,OAPO,EAaP;AACI,QAAA,KAAK,EAAC,YADV;AAEI,QAAA,KAAK,EAAC,YAFV;AAGI,QAAA,GAAG,EAAC,KAHR;AAII,QAAA,IAAI,EAAC,CAAC,MAAD,EAAQ,MAAR,EAAe,MAAf;AAJT,OAbO,EAmBP;AACI,QAAA,KAAK,EAAC,YADV;AAEI,QAAA,KAAK,EAAC,YAFV;AAGI,QAAA,GAAG,EAAC,KAHR;AAII,QAAA,IAAI,EAAC,CAAC,MAAD,EAAQ,MAAR,EAAe,MAAf;AAJT,OAnBO,EAyBP;AACI,QAAA,KAAK,EAAC,YADV;AAEI,QAAA,KAAK,EAAC,YAFV;AAGI,QAAA,GAAG,EAAC,KAHR;AAII,QAAA,IAAI,EAAC,CAAC,MAAD,EAAQ,MAAR,EAAe,MAAf;AAJT,OAzBO,EA+BP;AACI,QAAA,KAAK,EAAC,YADV;AAEI,QAAA,KAAK,EAAC,YAFV;AAGI,QAAA,GAAG,EAAC,KAHR;AAII,QAAA,IAAI,EAAC,CAAC,MAAD,EAAQ,MAAR,EAAe,MAAf;AAJT,OA/BO,EAqCP;AACI,QAAA,KAAK,EAAC,YADV;AAEI,QAAA,KAAK,EAAC,YAFV;AAGI,QAAA,GAAG,EAAC,KAHR;AAII,QAAA,IAAI,EAAC,CAAC,MAAD,EAAQ,MAAR,EAAe,MAAf;AAJT,OArCO,EA2CP;AACI,QAAA,KAAK,EAAC,YADV;AAEI,QAAA,KAAK,EAAC,YAFV;AAGI,QAAA,GAAG,EAAC,KAHR;AAII,QAAA,IAAI,EAAC,CAAC,MAAD,EAAQ,MAAR,EAAe,MAAf;AAJT,OA3CO,EAiDP;AACI,QAAA,KAAK,EAAC,YADV;AAEI,QAAA,KAAK,EAAC,YAFV;AAGI,QAAA,GAAG,EAAC,KAHR;AAII,QAAA,IAAI,EAAC,CAAC,MAAD,EAAQ,MAAR,EAAe,MAAf;AAJT,OAjDO,EAuDP;AACI,QAAA,KAAK,EAAC,YADV;AAEI,QAAA,KAAK,EAAC,YAFV;AAGI,QAAA,GAAG,EAAC,KAHR;AAII,QAAA,IAAI,EAAC,CAAC,MAAD,EAAQ,MAAR,EAAe,MAAf;AAJT,OAvDO;AArCJ,KAAP;AAoGF;AAnNa,CAAf","sourcesContent":["<template>\n<!-- 将拖拽滑块的过程和结束判断至于全局，开始判定放在对应滑块组件上 -->\n  <body @mousemove=\"dragging\" @mouseup=\"dragEnd\">\n    <transition name=\"fade\">\n      <div id=\"loginPart\" v-show=\"masked\">\n        <div id=\"loginMask\"/>\n        <Login v-show=\"select.login\">\n        </Login>\n        <div id=\"HKrecorder\" v-show=\"select.recorder\">\n          <div class=\"quit\" @click=\"recorderQuit\">\n            <img class=\"quitIcon\" src=\"./assets/quit.png\"/>\n          </div>\n          <br/>\n          <div id=\"recordInput\" >\n            <button id=\"switcher\" @click=\"switcher\">{{switcherText}}</button>\n            <transition name=\"fade\">\n              <button id=\"useTime\" @click=\"openTimeSetter\" v-show=\"switchStatus\">00:00</button>\n            </transition>\n            <!-- 时间选择器 -->\n            <transition name=\"fade\">\n              <div id=\"timeSetter\" v-show=\"timeSetter\">\n                <div class=\"quit\" @click=\"timeSetterQuit\">\n                  <img class=\"quitIcon\" src=\"./assets/quit.png\"/>\n                </div>\n                <div id=\"nums\">\n                  <div class=\"numShell\">\n                    <div class=\"dragBlock\" id=\"dragBlock1\" @mousedown=\"dragBegin\" >\n                      <div id=\"hour\" class=\"numCol\">\n                        <div class=\"numShell2\">\n                          <div class=\"numCell\"><span :style=\"preHourCSS\" class=\"number\">{{timer.preHour}}</span></div>\n                        </div>\n                        <div class=\"numShell2\">\n                          <div class=\"numCell\"><span :style=\"{opacity:timerCSS.currentHour_opacity}\" class=\"number\">{{timer.currentHour}}</span></div>\n                        </div>\n                        <div class=\"numShell2\">\n                          <div class=\"numCell\"><span :style=\"{opacity:timerCSS.nextHour_opacity}\" class=\"number\">{{timer.nextHour}}</span></div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                  <div class=\"numShell\">\n                    <div class=\"dragBlock\" draggable=\"true\">\n                      <div id=\"minute\" class=\"numCol\">\n                        <div class=\"numShell2\">\n                          <div class=\"numCell\"><span class=\"number\">{{timer.preMinute}}</span></div>\n                        </div>\n                        <div class=\"numShell2\">\n                         <div class=\"numCell\"><span class=\"number\">{{timer.currentMinute}}</span></div>\n                        </div>\n                        <div class=\"numShell2\">\n                          <div class=\"numCell\"><span class=\"number\">{{timer.nextMinute}}</span></div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                  <div class=\"numShell\">\n                    <div class=\"dragBlock\" draggable=\"true\">\n                      <div id=\"second\" class=\"numCol\">\n                        <div class=\"numShell2\">\n                          <div class=\"numCell\"><span class=\"number\">{{timer.preSecond}}</span></div>\n                        </div>\n                        <div class=\"numShell2\">\n                          <div class=\"numCell\"><span class=\"number\">{{timer.currentSecond}}</span></div>\n                        </div>\n                        <div class=\"numShell2\">\n                          <div class=\"numCell\"><span class=\"number\">{{timer.nextSecond}}</span></div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </transition>\n            <transition name=\"fade\">\n              <button id=\"upload\" @click=\"upload\" v-show=\"switchStatus\">上传</button>\n            </transition>\n          </div>\n          <div id=\"recorderBlank\">\n            <!-- 提交选择 -->\n            <transition-group name=\"bosses\">\n              <div class=\"HKcell\" v-show=\"switchStatus\" v-for=\"boss in bosses\" :key=\"boss.order\" >\n                <div class=\"HKcellShell\" @click=\"choose\">\n                  <img class=\"HKcellIcon\" :src=\"boss.iconSrc\"/>\n                </div>\n              </div>\n            </transition-group>\n            <!-- 统计展示 -->\n            <transition-group name=\"sortedBosses\">\n              <div class=\"HKcell\" v-show=\"!switchStatus\" v-for=\"sboss in sortedBosses\" :key=\"sboss.order\" >\n                <div class=\"HKcellShell\" @click=\"choose\">\n                  <img class=\"HKcellIcon\" :src=\"sboss.iconSrc\"/>\n                  <div class=\"numDiv\" v-show=\"!switchStatus\">{{sboss.order}}</div>\n                </div>\n              </div>\n            </transition-group>\n          </div>\n        </div>\n      </div>\n    </transition>\n    <div id=\"leftBar\">\n        <div id=\"loginCell\" class=\"cell\" @click=\"openLogin\">\n            <img id=\"user\" src=\"./assets/user.png\"/>\n            <!-- <img id=\"user_active\" src=\"../img/user_active.png\"/> -->\n        </div>\n        <div id=\"recorderCell\" class=\"cell\" @click=\"openRecorder\">\n          <img id=\"hollowKnightRecorder\" src=\"./assets/Pantheon_of_Hallownest.png\"/>\n        </div>\n    </div>\n    <div id=\"root\">\n        <div id=\"shell\">\n            <div id=\"title\">\n              <div id=\"title1\">Eferinte</div>\n              <img id=\"bird\" src=\"./assets/birdNichicho.png\"/>\n              <div id=\"title2\">の</div>\n              <div id=\"title3\">博客</div>\n              <div id=\"blank\"></div>\n            </div>\n        </div>\n        <br>\n        <div id=\"mainBox\">\n          <router-view></router-view>\n        </div>\n    </div>\n  </body>\n</template>\n<script>\nimport axios from 'axios' \nimport Login from './components/Login.vue'\nimport LeftBar from './components/LeftBar.vue'\nexport default {\n  components:{},\n  name: 'App',\n  methods: {\n    openLogin(){\n      this.masked = true;\n      this.select.login = true;\n    },\n    // 应该写个event获取到触发事件的具体部件，然后对其进行操作，实现对象化\n    loginQuit(){\n      this.masked = false;\n      this.select.login = false;\n    },\n    recorderQuit(){\n      this.masked = false;\n      this.select.recorder = false;\n    },\n    timeSetterQuit(){\n      this.timeSetter = false;\n    },        \n    openRecorder(){\n      this.masked = true;\n      this.select.recorder = true;\n    },\n    openTimeSetter(){\n      this.timeSetter = true;\n    },\n    // 根据鼠标事件获取拖动方向和距离\n    dragBegin(e){\n      this.draggable = true;\n      this.tempForMouseLocate = e.pageY;\n    },\n    dragging(e){\n      // console.log(e.target);\n      if(this.draggable){\n        if(e.pageY-this.tempForMouseLocate>0){\n          this.timerCSS.preHour_scaleValue++;\n        }else{\n          this.timerCSS.preHour_scaleValue--;\n        }\n        if(e.pageY - this.tempForMouseLocate>0){\n          document.getElementById(\"dragBlock1\").style.top = Math.min(e.pageY - this.tempForMouseLocate, 50)+\"px\";\n        }else{\n          document.getElementById(\"dragBlock1\").style.top = Math.max(e.pageY - this.tempForMouseLocate, -50)+\"px\";\n        }\n      }\n\n    },\n    dragEnd(e){\n      // console.log(e.target);\n      this.draggable = false;\n    },\n    switcher(){\n      if(this.switchStatus==false){\n        this.switchStatus = true;\n        this.switcherText = \"切换显示\"\n        // console.log(\"标准排序\")\n        // console.log(this.bosses)\n        // console.log(\"受击排序\")\n        // console.log(this.sortedBosses)\n      }else{\n        this.switchStatus = false;\n        this.switcherText = \"添加数据\"\n      }\n    },\n    choose(){\n      if(this.switchStatus){\n        // console.log(\"点击了\");\n      }\n    },\n    // 提交表单登录\n    login(){\n      console.log(\"发送请求\");\n\n      axios.get('http://localhost:8081/test')\n      .then((response)=>{\n          console.log(response.data[0])\n      })\n    }\n  },\n  created(){\n    var preUrl = \"/boss_sprites/statue/statue_\";\n    var testNum = this.bosses.length;\n    for(var i = 0;i<this.bosses.length;i++){\n      this.bosses[i].iconSrc = preUrl+this.bosses[i].name+\".png\";\n      this.bosses[i].order = i;\n      this.bosses[i].hitTimes = testNum;\n      testNum--;\n    }\n    //获取数据库数据，生产sortedBosses\n    //根据受击次数排序\n    this.sortedBosses = this.bosses.slice(0);\n    this.sortedBosses.sort((a,b)=>{return a.hitTimes-b.hitTimes})\n    console.log(this.sortedBosses)\n    \n    this.$router.push({\n        name:\"Home\",\n    })\n  },\n  computed:{\n    preHourCSS(){\n      // 由于不能直接对span进行缩放，故需要一个缩放等级到字体大小的映射过程\n      var scale = this.timerCSS.preHour_scaleValue;\n      var value = {\n        opacity: String(0.3*Math.atan(0.01*scale)+0.5),\n        fontSize: String(7*Math.atan(0.01*scale)+25)+'px' \n      }\n      return value;\n    }\n  },\n  data() {\n    return {\n      tempForMouseLocate:0,\n      offsetX:0,\n      offsetY:0,\n      draggable:false,\n      username:\"\",\n      paasword:\"\",\n      masked:false,\n      select:{\n        login:false,\n        recorder:false\n      },\n      timer:{\n        preHour:\"23\",\n        currentHour:\"00\",\n        nextHour:\"01\",\n        preMinute:\"59\",\n        currentMinute:\"00\",\n        nextMinute:\"01\",\n        preSecond:\"59\",\n        currentSecond:\"00\",\n        nextSecond:\"01\"\n      },\n      timerCSS:{\n        preHour_opacity:0.5,\n        currentHour_opacity:1,\n        nextHour_opacity:0.5,\n        // vue会将scale识别为用户自定义的函数\n        preHour_scaleValue:1,\n        currentHour_scale:1,\n        nextHour_scale:0.5\n      },\n      switchStatus:false,\n      timeSetter:false,\n      switcherText:\"添加数据\",\n      bosses:require(\"./bossData.json\").boss,\n      sortedBosses:[],\n      articles:[\n        {\n            title:\"test-title\",\n            brief:\"test-brief\",\n            aid:\"001\",\n            tags:[\"tag1\",\"tag2\",\"tag3\"]\n        },\n        {\n            title:\"test-title\",\n            brief:\"test-brief\",\n            aid:\"002\",\n            tags:[\"tag1\",\"tag2\",\"tag3\"]\n        },\n        {\n            title:\"test-title\",\n            brief:\"test-brief\",\n            aid:\"003\",\n            tags:[\"tag1\",\"tag2\",\"tag3\"]\n        },\n        {\n            title:\"test-title\",\n            brief:\"test-brief\",\n            aid:\"004\",\n            tags:[\"tag1\",\"tag2\",\"tag3\"]\n        },\n        {\n            title:\"test-title\",\n            brief:\"test-brief\",\n            aid:\"005\",\n            tags:[\"tag1\",\"tag2\",\"tag3\"]\n        },\n        {\n            title:\"test-title\",\n            brief:\"test-brief\",\n            aid:\"006\",\n            tags:[\"tag1\",\"tag2\",\"tag3\"]\n        },\n        {\n            title:\"test-title\",\n            brief:\"test-brief\",\n            aid:\"007\",\n            tags:[\"tag1\",\"tag2\",\"tag3\"]\n        },\n        {\n            title:\"test-title\",\n            brief:\"test-brief\",\n            aid:\"008\",\n            tags:[\"tag1\",\"tag2\",\"tag3\"]\n        },\n        {\n            title:\"test-title\",\n            brief:\"test-brief\",\n            aid:\"009\",\n            tags:[\"tag1\",\"tag2\",\"tag3\"]\n        },\n        {\n            title:\"test-title\",\n            brief:\"test-brief\",\n            aid:\"010\",\n            tags:[\"tag1\",\"tag2\",\"tag3\"]\n  